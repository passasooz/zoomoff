{
  "name": "ZoomOff",
  "description": "Escape from call in smart way!",
  "author": "Francesco Passanante",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "default_icon": {
    "16": "img/16.png",
    "32": "img/32.png",
    "128": "img/128.png"
  },
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-5As4+...'; object-src 'self'"
}